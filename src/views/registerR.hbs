<h1 class="registerHeader">Register</h1>
<a href="/" class="regHomeBut">Home</a>
<form action="/signup" method="post" class="registerForm">
    username <input type="text" name="username" id="username" required>
    age <input type="text" name="age" id="age" required>
    password <input type="password" name="password" id="password" required>
    Confirm Password<input type="password" name="confirmPassword" id="confirmPassword"required>
    dogname<input type="text" name="dogname" id="dogname" required>
    location <input type="text" name="location" id="location" required>
    <input type="submit" value="Create Accout">
</form>


